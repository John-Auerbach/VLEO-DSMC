# SPECIES AND MIXTURE -------------------------------------------------------------------------

# use local species file
species         species/air.species N2 O2

# define inflow gas mixture named atm
mixture 	    atm N2 frac 0.79
mixture		    atm O2 frac 0.21
mixture     	atm nrho ${nrho} vstream ${vx} 0.0 0.0 temp ${T}
mixture     	atm nrho 3.495979e+16 vstream ${vx} 0.0 0.0 temp ${T}
mixture     	atm nrho 3.495979e+16 vstream 7814.1 0.0 0.0 temp ${T}
mixture     	atm nrho 3.495979e+16 vstream 7814.1 0.0 0.0 temp 645.790588

# particle weighting: set target of 2e5 sim particles, calculate weighting factor
variable        Ns_target equal 2000000.0
variable        Vol       equal ${Lx}*${Ly}*${Lz}
variable        Vol       equal 2.6*${Ly}*${Lz}
variable        Vol       equal 2.6*2.2*${Lz}
variable        Vol       equal 2.6*2.2*2.2
variable        fnum      equal ${nrho}*${Vol}/${Ns_target}
variable        fnum      equal 3.495979e+16*${Vol}/${Ns_target}
variable        fnum      equal 3.495979e+16*12.584/${Ns_target}
variable        fnum      equal 3.495979e+16*12.584/2000000
global          fnum ${fnum} # global bc will be queried by each new particle
global          fnum 219966998680 

variable	    parts_per equal ${nrho}/${Ns_target}
variable	    parts_per equal 3.495979e+16/${Ns_target}
variable	    parts_per equal 3.495979e+16/2000000
